# Homepage Environment Variables
# Copy this file to your main .env and fill in the values

# === Service Hosts (IPs or hostnames) ===
# Only needed for services NOT in the same Docker network
PLEX_HOST=127.0.0.1
TAUTULLI_HOST=127.0.0.1

# === Remote Docker Host (Mobydick) ===
# IP do host Mobydick onde o Docker Socket Proxy est√° exposto na porta 2375
MOBYDICK_HOST=10.10.1.x

# === Media Services ===
PLEX_API_KEY=your_plex_api_key_here
TAUTULLI_API_KEY=your_tautulli_api_key_here

# === Media Management (*arr) ===
RADARR_API_KEY=your_radarr_api_key_here
SONARR_API_KEY=your_sonarr_api_key_here
PROWLARR_API_KEY=your_prowlarr_api_key_here
